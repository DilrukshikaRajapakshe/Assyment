package com.sliit.mtit.database;

public class payment {
	private String id;
	private String stockeID;
	private float price;
	private int amount;
	private float tot;
	
	public payment() {
		super();
	}
	public payment(String id, String stockeID, float price, int amount, float tot) {
		super();
		this.id = id;
		this.stockeID = stockeID;
		this.price = price;
		this.amount = amount;
		this.tot = tot;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getStockeID() {
		return stockeID;
	}
	public void setStockeID(String stockeID) {
		this.stockeID = stockeID;
	}
	public float getPrice() {
		return price;
	}
	public void setPrice(float price) {
		this.price = price;
	}
	public int getAmount() {
		return amount;
	}
	public void setAmount(int amount) {
		this.amount = amount;
	}
	public float getTot() {
		return tot;
	}
	public void setTot(float tot) {
		this.tot = tot;
	}
	@Override
	public String toString() {
		return "payment [id=" + id + ", stockeID=" + stockeID + ", price=" + price + ", amount=" + amount + ", tot="
				+ tot + "]";
	}
}
